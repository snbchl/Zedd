#include <rtthread.h>
#include <main.h>
#include <gpio.h>
#include <usart.h>
#include <stm32f1xx.h>
#include <stm32f1xx_hal_conf.h>
#include <stm32f1xx_it.h>


#include <rtdbg.h>

int main(void)
{
    int count = 1;
    SystemClock_Config(void);
    while (count++)
    {
        LOG_D("Hello RT-Thread!");
        rt_thread_mdelay(1000);
    }

    return RT_EOK;
}
